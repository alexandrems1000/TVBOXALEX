<?xml version="1.0" encoding="UTF-8"?>
<addon id="plugin.program.amoboxnotifications" version="4.0.0"
       name="TVBOXALEX Notifications" provider-name="TVBOXALEX">
  <requires>
  	<import addon="xbmc.python" version="2.1.0"/>
  	<import addon="script.module.addon.common" optional="true" />
  </requires>
  <extension point="xbmc.python.script" library="default.py">
    <provides>video executable</provides>
  </extension>
  <extension point="xbmc.service" library="service.py" start="startup">
  </extension>
  <extension point="xbmc.addon.metadata">
  	<summary>TVBOXALEX</summary>
  	<description>Recebe as mais recentes novidades da TVBOXALEX mal abres o Kodi. Cancelar subscrição nas definições do Addon.</description>
  	<disclaimer>Nós não assumimos qualquer responsabilidade por eventuais problemas este plugin possa causar.</disclaimer>
  	<language>en</language>
  	<platform>all</platform>
  	<website>http://</website>
  	<source>http://</source>
  	<email>tvboxalex@hotmail.com</email>
  </extension>
</addon>
